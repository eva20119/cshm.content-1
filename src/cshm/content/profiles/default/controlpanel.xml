<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="cshm.content">

    <configlet
        title="Office Related Setting"
        action_id="cshm.content.office"
        appId="cshm.content"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@officeControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
